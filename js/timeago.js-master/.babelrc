{
  "env": {
    "rollup": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-class-properties"
      ]
    },
    "babel": {
      "presets": ["env"],
      "plugins": [
        "transform-class-properties",
        "add-module-exports"
      ]
    }
  }
}
